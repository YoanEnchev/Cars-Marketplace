FLASK_APP="run.py"
FLASK_DEBUG=1

DB="postgresql"
DB_USER="my_user"
DB_HOST="localhost"
DB_POSTGRES_PORT=0
DB_NAME="my_db_name"
DB_TEST_NAME="my_db_name_test"
DB_PASSWORD="my_password"
DB_ENCODING="utf8"
DB_URL="${DB}://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_POSTGRES_PORT}/${DB_NAME}?client_encoding=${DB_ENCODING}"
DB_URL_TEST="${DB}://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_POSTGRES_PORT}/${DB_TEST_NAME}?client_encoding=${DB_ENCODING}"

SECRET_KEY="my secret key"